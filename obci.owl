            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;obci.owl#is_BCT_group_member_of"/>
                <owl:someValuesFrom rdf:resource="&obo;obci_0000024"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;obci.owl#acts_through"/>
