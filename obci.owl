        <BCT_taxonomy_code rdf:datatype="&xsd;double">8.7</BCT_taxonomy_code>
        <definition>Set easy-to-perform tasks, making them increasingly difficult, but achievable, until behavior is performed.</definition>
        <example_of_usage>Ask the person to walk for 100 yards a day for the first week, then half a mile a day after they have successfully achieved 100 yards, then two miles a day after they have successfully achieved one mile.</example_of_usage>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/obci_0000108 -->

    <owl:Class rdf:about="&obo;obci_0000108">
